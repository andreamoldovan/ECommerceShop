@page  "/{id:int?}"@model ECommerceShop.Pages.Coffeee.IndexModel@{    ViewData["Title"] = "Index";}<h1>Coffee®</h1><p>    <a asp-page="Create">Choose Your Coffee</a></p><table class="table">    <thead>        <tr>            <th>                @Html.DisplayNameFor(model => model.Coffee[0].Name)            </th>            <th>                @Html.DisplayNameFor(model => model.Coffee[0].Feature)            </th>            <th>                @Html.DisplayNameFor(model => model.Coffee[0].Price)            </th>            <th>                @Html.DisplayNameFor(model => model.Coffee[0].ArrivingDate)            </th>            <th>                @Html.DisplayNameFor(model => model.Coffee[0].Customer.CustomerName)            </th>            <th>                @Html.DisplayNameFor(model => model.Coffee[0].CoffeeCategories)                </th>            <th></th>        </tr>    </thead>    <tbody>        @foreach (var item in Model.CoffeeD.Coffeee)        {            string selectedRow = "";            if (item.ID == Model.CoffeeID)            {                selectedRow = "table-success";            }        <tr class="@selectedRow">            <td>                @Html.DisplayFor(modelItem => item.Name)            </td>            <td>                @Html.DisplayFor(modelItem => item.Feature)            </td>            <td>                @Html.DisplayFor(modelItem => item.Price)            </td>            <td>                @Html.DisplayFor(modelItem => item.ArrivingDate)            </td>            <td>                @Html.DisplayFor(modelItem => item.Customer.CustomerName)            </td>            <td>                @{                    foreach (var category in item.CoffeeCategories)                    {                        @category.Category.ID @: @category.Category.CategoryName <br />                    }                }            </td>            <td>                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>            </td>        </tr>}    </tbody></table>